        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".materialdesign.recycler.RecyclerViewActivity"></activity>
        <activity android:name=".materialdesign.nested.NestedScrollActivity" />
        <activity android:name=".materialdesign.BehaviorActivity" />
        <activity android:name=".status.StatusBarActivity1" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>