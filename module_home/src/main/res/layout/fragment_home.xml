<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btnNoResult"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="NoResultActivity" />


    <Button
        android:id="@+id/btnResultClient"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ResultClientActivity" />

    <Button
        android:id="@+id/btnEventBus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="EventBus" />


    <Button
        android:id="@+id/btnInterButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="InterActivity" />

    <Button
        android:id="@+id/btnInject"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="InjectActivity" />
</LinearLayout>